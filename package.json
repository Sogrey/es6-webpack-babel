{
  "name": "es6-webpack-babel",
  "version": "0.0.1",
  "description": "A simple ES6 Webpack Babel Starter Kit.",
  "scripts": {
    "start": "node_modules/.bin/webpack-dev-server --output-public-path=/dist/  --mode development --progress --open --hot",
    "build:md": "webpack --mode=development --progress --color",
    "build:mp": "webpack --mode=production --progress --color",
    "build": "npm run build:r & npm run build:mp",
    "build:r": "node tools/rollup-build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sogrey/es6-webpack-babel.git"
  },
  "keywords": [
    "ES6",
    "webpack",
    "babel"
  ],
  "author": "Sogrey",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.12.13",
    "babel-loader": "^8.2.2",
    "babel-preset-env": "^1.7.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "rollup": "^0.67.3",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^3.4.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.12.13",
    "core-js": "^3.8.3"
  },
  "bugs": {
    "url": "https://github.com/Sogrey/es6-webpack-babel/issues"
  },
  "homepage": "https://github.com/Sogrey/es6-webpack-babel#readme"
}